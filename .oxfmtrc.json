{
  "$schema": "https://raw.githubusercontent.com/oxc-project/oxc/main/crates/oxfmt/assets/configuration.md",
  "indentStyle": "space",
  "indentWidth": 2,
  "lineWidth": 100,
  "semi": false,
  "singleQuote": true,
  "trailingComma": "es5",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "quoteProps": "as-needed",
  "jsxSingleQuote": false,
  "proseWrap": "preserve"
}
